{
  "title": "Cypress.io",
  "options": {
    "controls": true,
    "progress": true,
    "history": true,
    "center": true,
    "slideNumber": true,
    "touch": true,
    "hideAddressBar": true,
    "mouseWheel": true
  },
  "plugins": {
    "search": true,
    "zoom":   false,
    "menu":   true,
    "notes":  true,
    "highlightjs": true
  },
  "slides": [
    {"path": "index.md", "attrs": {"class":"intro"}},
    [
      {"path": "01-introduccion/01-titulo.md", "attrs": {"class":"bg-dark"}},
      {"path": "01-introduccion/02-automatizar.md", "attrs": {"class":"bg-grey"}},
      {"path": "01-introduccion/03-cypress.md", "attrs": {"class":"bg-dark"}},
      {"path": "01-introduccion/04-cypress-framework.md", "attrs": {"class":"bg-dark"}},
      {"path": "01-introduccion/05-e2e.md", "attrs": {"class":"bg-dark"}},
      {"path": "01-introduccion/05-e2e1.md", "attrs": {"class":"bg-dark"}},
      {"path": "01-introduccion/07-cypress-caracteristicas.md", "attrs": {"class":"bg-dark"}},
      {"path": "01-introduccion/08-comparacion-frameworks.md", "attrs": {"class":"bg-dark"}},
      {"path": "01-introduccion/09-cita.md", "attrs": {"class":"bg-img"}}
    ],
    [
      "02-instalacion/*.md"
    ],
    [
      "03-modos_ejecucion/*.md"
    ],
    [
      "04-primeros_tests/*.md"
    ],
    [
      "05-selectores_elementos/*.md"
    ],
    [
      "06-ejercicio_practico1/*.md"
    ],
    [
      "07-otros_comandos/*.md"
    ],
    [
      "08-ejercicio_practico2/*.md"
    ],
    [
      "09-comandos_custom/*.md"
    ],
    {"path": "last.md", "attrs": {"class":"last bg-red"}}
  ]
}
