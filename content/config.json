{
  "title": "Cypress.io",
  "options": {
    "controls": true,
    "progress": true,
    "history": true,
    "center": true,
    "slideNumber": true,
    "touch": true,
    "hideAddressBar": true,
    "mouseWheel": true
  },
  "plugins": {
    "search": true,
    "zoom":   true,
    "menu":   true,
    "notes":  true,
    "highlightjs": true
  },
  "slides": [
    {"path": "index.md", "attrs": {"class":"intro"}},
    [
      "01-introduccion/*.md"
    ],
    [
      "02-instalacion/*.md"
    ],
    [
      "03-modos_ejecucion/*.md"
    ],
    [
      "04-primeros_tests/*.md"
    ],
    [
      "05-selectores_elementos/*.md"
    ],
    [
      "06-ejercicio_practico1/*.md"
    ],
    [
      "07-otros_comandos/*.md"
    ],
    [
      "08-ejercicio_practico2/*.md"
    ],
    [
      "09-comandos_custom/*.md"
    ],
    "end.html"
  ]
}
